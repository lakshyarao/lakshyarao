import java.util.Scanner;

public class FactorialDoWhile {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int num = scanner.nextInt();
        scanner.close();
        
        int factorial = 1;
        if (num > 0) {
            int i = num;
            do {
                factorial *= i;
                i--;
            } while (i > 0);
        }
        
        System.out.println(factorial);
    }
}
